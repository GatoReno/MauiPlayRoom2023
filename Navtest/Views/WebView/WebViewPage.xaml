<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Navtest.Views.WebView.WebViewPage"
             Title="WebViewPage">
    <VerticalStackLayout
        VerticalOptions="FillAndExpand"
        >
        <WebView
            x:Name="bootstrapwebviewer"
            HeightRequest="500"
            VerticalOptions="FillAndExpand"
            Source="https://startbootstrap.com/previews/sb-admin"
            />
        <Button
            Margin="10,10"
            Text="open launcher"
            Clicked="Button_Clicked"
            />
    </VerticalStackLayout>
</ContentPage>
